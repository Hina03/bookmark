    "@types/react-dom": "^18",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "husky": "^8.0.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
